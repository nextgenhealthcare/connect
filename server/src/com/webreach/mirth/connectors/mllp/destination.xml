<connectorMetaData>
	<name>LLP Sender</name>
	<author>WebReach, Inc.</author>
	<pluginVersion>@mirthversion</pluginVersion>
	<mirthVersion>1.7.1</mirthVersion>
	<url>http://extensions.mirthproject.org</url>
	<description>This connector allows Mirth to send messages to MLLP compatabile servers. The connector can automatically retry failed messages and handle ACK responses.</description>
	<clientClassName>com.webreach.mirth.connectors.mllp.LLPSender</clientClassName>
	<serverClassName>com.webreach.mirth.connectors.mllp.MllpConnector</serverClassName>
	<sharedClassName>com.webreach.mirth.connectors.mllp.LLPSenderProperties</sharedClassName>
	<library type="client" path="mllp-client.jar" />
	<library type="shared" path="mllp-shared.jar" />
	<library type="server" path="mllp-server.jar" />
	<transformers></transformers>
	<protocol>mllp</protocol>
	<type>DESTINATION</type>
	<enabled>true</enabled>
	<mule-properties>
		<property name="connector">com.webreach.mirth.connectors.mllp.MllpConnector</property>
		<property name="dispatcher.factory">com.webreach.mirth.connectors.mllp.MllpMessageDispatcherFactory</property>
		<property name="message.receiver">com.webreach.mirth.connectors.mllp.MllpMessageReceiver</property>
		<property name="message.adapter">com.webreach.mirth.connectors.mllp.MllpMessageAdapter</property>
		<property name="inbound.transformer">org.mule.transformers.simple.ByteArrayToString</property>
		<property name="outbound.transformer">org.mule.transformers.simple.StringToByteArray</property>
		<property name="endpoint.builder">org.mule.impl.endpoint.SocketEndpointBuilder</property>
	</mule-properties>
</connectorMetaData>