<connectorMetaData path="ws">
	<name>Web Service Listener</name>
	<author>Mirth Corporation</author>
	<pluginVersion>@mirthversion</pluginVersion>
	<mirthVersion>1.8.2</mirthVersion>
	<url>http://www.mirthcorp.com</url>
	<description>This connector allows Mirth to listen on an HTTP port for incoming Web Service calls. This connector also provides a WSDL for SOAP clients to use.</description>
	<clientClassName>com.webreach.mirth.connectors.ws.WebServiceListener</clientClassName>
	<sharedClassName>com.webreach.mirth.connectors.ws.WebServiceListenerProperties</sharedClassName>
	<serverClassName>com.webreach.mirth.connectors.ws.WebServiceConnector</serverClassName>
	<serviceClassName>com.webreach.mirth.connectors.ws.WebServiceConnectorService</serviceClassName>
	<transformers></transformers>
	<protocol>ws</protocol>
	<type>SOURCE</type>
	<enabled>true</enabled>
	<library path="ws-server.jar">
		<type>SERVER</type>
	</library>
	<library path="ws-client.jar">
		<type>CLIENT</type>
	</library>
	<library path="ws-shared.jar">
		<type>SHARED</type>
	</library>
	<mule-properties>
		<property name="connector">com.webreach.mirth.connectors.ws.WebServiceConnector</property>
		<property name="dispatcher.factory">com.webreach.mirth.connectors.ws.WebServiceMessageDispatcherFactory</property>
		<property name="message.receiver">com.webreach.mirth.connectors.ws.WebServiceMessageReceiver</property>
		<property name="message.adapter">com.webreach.mirth.connectors.ws.WebServiceMessageAdapter</property>
	</mule-properties>
</connectorMetaData>