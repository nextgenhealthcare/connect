<connectorMetaData path="com.webreach.mirth.connectors.js">
	<name>JavaScript Reader</name>
	<author>WebReach, Inc.</author>
	<pluginVersion>@mirthversion</pluginVersion>
	<mirthVersion>1.7.1</mirthVersion>
	<url>http://extensions.mirthproject.org</url>
	<description>This connector allows Mirth to execute arbitrary JavaScript to pull in messages to a channel.</description>
	<clientClassName>com.webreach.mirth.connectors.js.JavaScriptReader</clientClassName>
	<serverClassName>com.webreach.mirth.connectors.js.JavaScriptConnector</serverClassName>
	<sharedClassName>com.webreach.mirth.connectors.js.JavaScriptReaderProperties</sharedClassName>
	<library type="CLIENT" path="js-client.jar" />
	<library type="SHARED" path="js-shared.jar" />
	<library type="SERVER" path="js-server.jar" />
	<transformers>ResultMapToXML</transformers>
	<protocol>js</protocol>
	<type>SOURCE</type>
	<enabled>true</enabled>
	<mule-properties>
		<property name="connector">com.webreach.mirth.connectors.js.JavaScriptConnector</property>
		<property name="dispatcher.factory">com.webreach.mirth.connectors.js.JavaScriptMessageDispatcherFactory</property>
		<property name="message.receiver">com.webreach.mirth.connectors.js.JavaScriptMessageReceiver</property>
		<property name="message.adapter">com.webreach.mirth.connectors.js.JavaScriptMessageAdapter</property>
	</mule-properties>
</connectorMetaData>