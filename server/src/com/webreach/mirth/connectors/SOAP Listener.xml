<connectorMetaData>
  <name>SOAP Listener</name>
  <author>WebREACH, Inc.</author>
  <pluginVersion>1.6.0</pluginVersion>
  <mirthVersion>1.6.0</mirthVersion>
  <url>http://www.mirthproject.org</url>
  <clientClassName>com.webreach.mirth.connectors.soap.SOAPListener</clientClassName>
  <serverClassName>com.webreach.mirth.connectors.soap.axis.AxisConnector</serverClassName>
  <sharedClassName>com.webreach.mirth.connectors.soap.SOAPListenerProperties</sharedClassName>
  <transformers>SOAPRequestToString</transformers>
  <protocol>axis</protocol>
  <type>LISTENER</type>
  <enabled>true</enabled>
  <mule-properties>
      <property name="connector">com.webreach.mirth.connectors.soap.axis.transport.http.SoapConnector</property>
      <property name="dispatcher.factory">org.mule.providers.http.HttpClientMessageDispatcherFactory</property>
      <property name="message.receiver">org.mule.providers.http.HttpMessageReceiver</property>
      <property name="message.adapter">org.mule.providers.http.HttpMessageAdapter</property>
      <property name="outbound.transformer">org.mule.providers.http.transformers.ObjectToHttpClientMethodRequest</property>
      <property name="response.transformer">com.webreach.mirth.connectors.soap.axis.transport.http.transformers.UMOMessageToHttpResponse</property>
  </mule-properties>
</connectorMetaData>