<pluginMetaData path="com.webreach.mirth.plugins.pdfviewer">
	<name>PDF Viewer</name>
	<author>WebReach, Inc.</author>
	<pluginVersion>@mirthversion</pluginVersion>
	<mirthVersion>1.7.1</mirthVersion>
	<url>http://extensions.mirthproject.org</url>
	<description>This plugin provides PDF attachment viewing capability in message browser.</description>
	<enabled>true</enabled>
	<extensionPoint>
		<name>PDF Viewer</name>
		<type>ATTACHMENT_VIEWER</type>
		<className>com.webreach.mirth.plugins.pdfviewer.PDFViewer</className>
		<mode>CLIENT</mode>
	</extensionPoint>
	<library path="pdfviewer-client.jar">
		<type>CLIENT</type>	
	</library>
</pluginMetaData>